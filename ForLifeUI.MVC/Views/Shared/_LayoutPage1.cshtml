                <div class="header-top-bar">
                    <div class="container">
                        <div class="row">
                            <div class="col-lg-2 col-md-2 col-sm-2 hidden-xs">
                                <div class="logo-area">
                                    <a href="index.htm"><img class="img-responsive" src="~/img/logo2t.png" alt="logo" style="width:90px;height:55px"></a>
                                </div>
                            </div>
                            <div class="col-lg-10 col-md-10 col-sm-10 col-xs-12">
                                <ul class="profile-notification">
                                    <li>
                                        <div class="apply-btn-area">
                                            <a class="apply-now-btn" href="#" id="Admin-button">Admin Girişi</a>
