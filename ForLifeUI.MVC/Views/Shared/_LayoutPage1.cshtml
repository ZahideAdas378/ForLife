                                                <form>
                                                    <input class="form-control" type="text" placeholder="Name">
                                                    <input class="form-control" type="password" placeholder="Password">
                                                    <button class="btn-login" type="submit" value="Login">Login</button>
                                                    <div class="remember-lost">
                                                        <div class="checkbox">
                                                            <label><input type="checkbox"> Remember me</label>
                                                        </div>
                                                    </div>
                                                    <button class="cross-btn form-cancel" type="submit" value=""><i class="fa fa-times" aria-hidden="true"></i></button>
                                                </form>
                                            </div>
                                        </div>
                                    </li>

                                    <li>
                                        <div class="apply-btn-area">
                                            <a class="apply-now-btn" href="#" id="Patient-button">Hasta Girişi</a>
                                            <div class="login-form" id="login-form" style="display: none;">
                                                <h2>Login</h2>
                                                <form>
                                                    <input class="form-control" type="text" placeholder="Name">
                                                    <input class="form-control" type="password" placeholder="Password">
                                                    <button class="btn-login" type="submit" value="Login">Login</button>

                                                    <div class="remember-lost">
                                                        <div class="checkbox">
                                                            <label><input type="checkbox"> Remember me</label>
                                                        </div>
                                                        <a class="lost-password" href="#">Lost Your Password?</a>
                                                    </div>
                                                    <button class="cross-btn form-cancel" type="submit" value=""><i class="fa fa-times" aria-hidden="true"></i></button>
                                                </form>
                                            </div>
                                        </div>
                                    </li>
                                    <li>
                                        <div class="apply-btn-area">
                                            <a class="apply-now-btn" href="#" id="Donor-button">Donor Girişi</a>
                                            <div class="login-form" id="login-form" style="display: none;">
                                                <h2>Login</h2>
                                                <form>
                                                    <input class="form-control" type="text" placeholder="Name">
                                                    <input class="form-control" type="password" placeholder="Password">
                                                    <button class="btn-login" type="submit" value="Login">Login</button>

                                                    <div class="remember-lost">
                                                        <div class="checkbox">
                                                            <label><input type="checkbox"> Remember me</label>
                                                        </div>
                                                        <a class="lost-password" href="#">Lost Your Password?</a>
                                                    </div>
                                                    <button class="cross-btn form-cancel" type="submit" value=""><i class="fa fa-times" aria-hidden="true"></i></button>
                                                </form>
                                            </div>
                                        </div>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="main-menu-area bg-primaryText" id="sticker">
                    <div class="container">
                        <nav id="desktop-nav">
                            <ul>
                                <li><a href="/Home/Index">Anasayfa</a></li>
                                <li><a href="/About/Index">Hakkımızda</a></li>
                                <li><a href="advancedSearch.htm">Gelişmiş Profil Arama</a></li>
                                <li><a href="blog.htm">Yasal Düzenlemeler</a></li>
                                <li><a href="contact.htm">İletişim</a></li>
                            </ul>
                        </nav>
                    </div>
                </div>
            </div>
        </header>
       

    <div>
        @RenderBody()
